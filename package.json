{
  "name": "water-quality",
  "version": "0.1.0",
  "main": "src/index.ts",
  "description": "Backend for water-quality",
  "scripts": {
    "lint": "eslint src/* --ext .ts",
    "service": "nodemon",
    "start": "DEBUG=WaterQuality:* ts-node ./src/index",
    "release": "standard-version",
    "test:local": "jest --setupFiles dotenv/config --ci -i",
    "test:ci": "jest --ci -i",
    "pub": "nodemon --exec \"DEBUG=WQ:* ts-node -r dotenv/config src/pub.ts\""
  },
  "author": "AnthonyLzq <sluzquinosa@uni.pe>",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/node-cron": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-unit": "^0.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "debug": "^4.3.4",
    "firebase-admin": "^11.4.1",
    "mqtt": "^4.3.7",
    "node-cron": "^3.0.2",
    "socket.io": "^4.5.4",
    "zod": "^3.20.2"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.test.ts"
    ],
    "exec": "DEBUG=WaterQuality:* npx ts-node -r dotenv/config ./src/index"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnthonyLzq/water-quality.git"
  },
  "keywords": [
    "mqqt"
  ],
  "bugs": {
    "url": "https://github.com/AnthonyLzq/water-quality/issues"
  },
  "homepage": "https://github.com/AnthonyLzq/water-quality#readme"
}
